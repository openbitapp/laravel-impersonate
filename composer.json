{
  "name": "openbitapp/laravel-passport-impersonate",
  "description": "Laravel Passport Impersonate is a plugin that allows to you to authenticate as your users using JWT tokens with Laravel Passport.",
  "type": "library",
  "keywords": [
    "laravel",
    "package",
    "plugin",
    "impersonate",
    "impersonation",
    "user",
    "auth",
    "laravel-package",
    "laravel-plugin",
    "passport",
    "laravel-passport"
  ],
  "require": {
    "php": "^7.4 | ^8.0",
    "laravel/framework": "^8.0",
    "laravel/passport": "^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 | ^8.0 | ^9.0",
    "mockery/mockery": "^1.3.3",
    "orchestra/testbench": "^4.0 | ^5.0 | ^6.0",
    "orchestra/database": "^4.0 | ^5.0 | ^6.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Marceau Casals",
      "email": "marceau@casals.fr"
    },
    {
      "name": "rickycezar",
      "email": "barros.henrique.c@gmail.com"
    },
    {
      "name": "Andrea Martini",
      "email": "andrea.martini@bitapp.it"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bitapp\\Impersonate\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bitapp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bitapp\\Impersonate\\ImpersonateServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}